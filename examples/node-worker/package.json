{
  "name": "@examples/node-worker",
  "version": "0.0.0",
  "type": "module",
  "bundler": {
    "rsc": {
      "additionalModuleDirs": [
        "../node_modules",
        "../../node_modules"
      ]
    },
    "ssr": {
      "additionalModuleDirs": [
        "../node_modules",
        "../../node_modules"
      ]
    }
  },
  "scripts": {
    "build": "rsc-webpack-playground build && tsc",
    "dev": "rsc-webpack-playground dev -- node --watch --conditions webpack server.js | gsed -u 's/\\x1Bc//g'",
    "start": "node --conditions webpack server.js"
  },
  "dependencies": {
    "@remix-run/router": "1.4.0",
    "compression": "1.7.4",
    "express": "4.18.2",
    "isbot": "3.6.7",
    "react": "0.0.0-experimental-3706edb81-20230308",
    "react-dom": "0.0.0-experimental-3706edb81-20230308",
    "react-server-dom-webpack": "0.0.0-experimental-3706edb81-20230308",
    "thread-stream": "2.3.0"
  },
  "devDependencies": {
    "@rsc-webpack-playground/cli": "0.0.0",
    "@types/express": "4.17.17",
    "@types/node": "18.15.3",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "concurrently": "7.6.0",
    "typescript": "4.9.5"
  }
}
